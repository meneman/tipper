
                $('#SetScoreMasdsadodal').on('show.bs.modal', function (event) {
          var button = $(event.relatedTarget) // Button that triggered the modal
        var matchId = button.data('match');

          $.get('ajax/list.ajax.php', { res: 'matches', bid: matchId}).done( function (data){
                       match =$.parseJSON(data);
          var modal = $(this)
          modal.find('.modal-title').text('New message to ')
                        $("#SetScoreInputs").append("<input type=text name=ScoreOne class=form-control id=tOne placeholder='"+match.name_first+" Tore' > <input type=text name=ScoreTwo class=form-control id=tTwo placeholder='"+match.name_secound+" Tore' >");
                        

                    });
          

        })